{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Response schema for a fmb query by icd10 code",
  "type": "object",
  "properties": {
    "icd10Code": {
      "type": "string"
    },
    "forms": {
      "type": "array",
      "minItems": 0,
      "uniqueItems": false,
      "additionalItems": true,
      "items": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "enum": [
              "FORM2",
              "FORM4",
              "FORM5",
              "FORM8B"
            ]
          },
          "content": {
            "type": "array",
            "minItems": 0,
            "uniqueItems": false,
            "additionalItems": true,
            "items": {
              "type": "object",
              "properties": {
                "heading": {
                  "type": "string",
                  "enum": [
                    "FALT2_SPB",
                    "FALT2_GENERAL",
                    "FALT4",
                    "FALT5",
                    "FALT8B"
                  ]
                },
                "text": {
                  "type": "string"
                },
                "list": {
                  "type": "array",
                  "minItems": 0,
                  "uniqueItems": false,
                  "additionalItems": true,
                  "items": {
                    "type": "string"
                  }
                }
              },
              "additionalProperties": false,
              "required": [
                "heading"
              ]
            }
          }
        },
        "additionalProperties": false
      }
    }
  },
  "additionalProperties": false,
  "required": [
    "icd10Code",
    "forms"
  ]
}