  <build>
    <plugins>
      <plugin>
        <groupId>org.dstovall</groupId>
        <artifactId>onejar-maven-plugin</artifactId>
        <version>1.3.0</version>
        <executions>
            <execution>
                <configuration>
                    <mainClass>se.inera.certificate.mc2wc.Main</mainClass>
                </configuration>
                <goals>
                    <goal>one-jar</goal>
                </goals>
            </execution>
        </executions>
      </plugin>
      <plugin>
        <groupId>org.codehaus.mojo</groupId>
        <artifactId>exec-maven-plugin</artifactId>
        <version>1.3</version>
        <configuration>
          <mainClass>se.inera.certificate.mc2wc.Main</mainClass>
          <arguments>
            <argument>--configFile=${basedir}/local-test.properties</argument>
          </arguments>
        </configuration>
      </plugin>
    </plugins>
  </build>
